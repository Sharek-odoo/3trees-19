<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="branch_inherit_account_auto_transfer"
            model="ir.ui.view">
            <field name="name">Inherit Branch Account Transfer</field>
            <field name="model">account.transfer.model</field>
            <field name="inherit_id" ref="account_transfer.view_transfer_model_form" />
            <field name="arch" type="xml">
                <field name="frequency" position="after">
                    <field name="branch_id"
                        options='{"no_open": True,"no_create": 1, "no_create_edit": 1}'
                        required="1" />
                </field>
            </field>
        </record>
    </data>
</odoo>