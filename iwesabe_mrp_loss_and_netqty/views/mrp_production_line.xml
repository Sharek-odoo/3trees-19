<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_mrp_production_form_inherited" model="ir.ui.view">
        <field name="name">mrp.production.line.form.inherited</field>
        <field name="model">mrp.production</field>
        <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='move_raw_ids']/list/field[@name='product_uom_qty']" position="before">
                <field name="net_quantity"/>
                <field name="loss_percentage"/>
            </xpath>
            <xpath expr="//field[@name='move_raw_ids']/list/field[@name='product_uom_qty']" position="attributes">
                <attribute name="force_save">1</attribute>
            </xpath>
        </field>
    </record>
</odoo>





