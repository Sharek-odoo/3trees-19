<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="view_stock_quant_tree_inherit_branch">
        <field name="name">stock.quant.tree.inherit.branch</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree"></field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='location_id']" position="before">
                <field name="branch_id"/>
            </xpath>
        </field>
    </record>

    <record model="ir.ui.view" id="view_stock_quant_tree_editable_inherit_branch">
        <field name="name">stock.quant.tree.editable.inherit.branch</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" 
        ref="stock.view_stock_quant_tree_inventory_editable"></field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='location_id']" position="before">
                <field name="branch_id"/>
            </xpath>
        </field>
    </record>

</odoo>
